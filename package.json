{
  "name": "@ballerine/workflows-service",
  "private": false,
  "version": "0.4.17",
  "description": "workflow-service",
  "scripts": {
    "spellcheck": "cspell \"*\"",
    "setup": "npm run docker:db && npm run db:clean && npm run db:migrate-save && npm run seed",
    "format": "prettier --write . '!**/*.{md}'",
    "format:check": "prettier --check . '!**/*.{md}'",
    "lint": "eslint . --fix",
    "start": "node ./dist/index.js",
    "dev": "npm run start:watch",
    "prod": "npm run db:migrate-up && node dist/index.js",
    "start:watch": "tsx watch ./src/index.ts",
    "start:debug": "node --inspect=0.0.0.0:9229 --loader tsx ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "tsc --path=tsconfig.build.json",
    "test": "jest --verbose",
    "test:unit": "cross-env SKIP_DB_SETUP_TEARDOWN=true jest --testRegex '.*\\.unit\\.test\\.ts$'",
    "test:integration": "jest --testRegex '.*\\.intg\\.test\\.ts$' --runInBand",
    "test:e2e": "jest --testRegex '.*\\.e2e\\.test\\.ts$'",
    "test:watch": "jest --verbose --watch",
    "seed": "tsx scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "tsx scripts/clean.ts",
    "db:reset": "npm run db:clean && npm run db:migrate-up",
    "db:reset:dev": "npm run db:migrate-up && npm run db:reset && npm run seed",
    "db:init": "npm run db:migrate-save -- --name 'initial version' && npm run db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "json-schema:generate": "json2ts src/workflow/schemas/default-context-schema.json src/workflow/schemas/context.d.ts",
    "docker:db": "docker compose -f docker-compose.db.yml up -d --wait",
    "docker:build": "docker build .",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down --volumes",
    "upload-sourcemaps": "tsx scripts/upload-sourcemaps.ts",
    "prepare": "npm run prisma:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.325.0",
    "@aws-sdk/lib-storage": "3.325.0",
    "@ballerine/common": "0.5.6",
    "@ballerine/workflow-node-sdk": "0.4.3",
    "@faker-js/faker": "^7.6.0",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/passport": "^2.3.0",
    "@fastify/request-context": "^5.0.0",
    "@fastify/session": "^10.4.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/type-provider-typebox": "^3.4.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/devtools-integration": "^0.1.4",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "^10.0.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/testing": "^9.3.12",
    "@prisma/client": "^4.13.0",
    "@sentry/cli": "^2.17.5",
    "@sentry/integrations": "^7.52.1",
    "@sentry/node": "^7.52.1",
    "@sinclair/typebox": "^0.30.4",
    "@t3-oss/env-core": "^0.3.1",
    "@types/tmp": "^0.2.3",
    "accesscontrol": "^2.2.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.4.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-session": "^2.0.0",
    "express-cookie": "^4.17.1",
    "fastify": "^4.21.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^4.5.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "multer-s3": "3.0.1",
    "nest-access-control": "2.2.0",
    "nestjs-cls": "^3.5.0",
    "nestjs-prisma": "0.20.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.15.0",
    "qs": "^6.11.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "4.6.2",
    "tmp": "^0.2.1",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cspell/cspell-types": "^6.31.1",
    "@nestjs/cli": "9.3.0",
    "@nestjs/swagger": "6.2.1",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "4.17.9",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.14.6",
    "@types/normalize-path": "3.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "^1.0.35",
    "@types/qs": "^6.9.7",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "cross-env": "^7.0.3",
    "cspell": "^6.31.2",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.6.2",
    "jest-mock-extended": "^3.0.5",
    "json-schema-to-typescript": "^13.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^2.8.4",
    "prisma": "4.13.0",
    "supertest": "4.0.2",
    "testcontainers": "^9.8.0",
    "ts-jest": "29.1.1",
    "tsx": "^3.12.7",
    "type-fest": "0.11.0",
    "typescript": "^4.9.3"
  }
}
